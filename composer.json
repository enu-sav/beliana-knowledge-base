{
    "name": "drupal/beliana-knowledge-base",
    "description": "Beliana Knowledge Base",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "dompdf/dompdf": "^3.1",
        "drupal/ai": "^1.0",
        "drupal/ai_provider_openai": "^1.0",
        "drupal/ai_provider_perplexity": "^1.0@beta",
        "drupal/config_filter": "^2.7",
        "drupal/config_ignore": "^3.3",
        "drupal/core-composer-scaffold": "^10.4",
        "drupal/core-project-message": "^10.4",
        "drupal/core-recommended": "^10.4",
        "drupal/environment_indicator": "^4.0",
        "drupal/gin": "^4.1",
        "drupal/gin_toolbar": "^2.0",
        "drupal/inline_entity_form": "3.0-RC19",
        "drupal/jsonapi_extras": "^3.26",
        "drupal/jsonapi_permission_access": "^1.0",
        "drupal/r4032login": "^2.2",
        "drupal/remove_reset_password": "^1.0",
        "drush/drush": "^12.5",
        "vlucas/phpdotenv": "^5.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true,
        "patches": {
            "drupal/gin": {
                "3345842 - Allow removing the 'Back to site' link on admin-only sites": "https://git.drupalcode.org/project/gin/-/merge_requests/231.diff"
            },
            "drupal/inline_entity_form": {
                "3088901 - Add more than two items with simple inline entity form widget": "./patches/3088901-16-modified.patch"
            }
        }
    }
}
